{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "prebuild": "rm -rf /dist",
    "build": "tsc --build",
    "start": "ENV=dev nodemon ./src/server.ts",
    "start:prod": "PORT=3000 ENV=prod node dist/server.js",
    "start:preview": "ENV=preview node dist/server.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:drop-schema": "npm run typeorm schema:drop -- -d ./src/database/app-data-source.ts",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/database/app-data-source.ts",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
    "typeorm:generate-migration": "npm run typeorm -- -d ./src/database/app-data-source.ts migration:generate ./src/database/migrations/$npm_config_name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  }
}
